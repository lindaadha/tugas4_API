{
  "name": "tugas4_api",
  "version": "1.0.0",
  "description": "tugas untuk API automation menggunakan mocha, supertest",
  "main": "index.js",
  "scripts": {
    "test:user": "mocha test/suite/user.suite.js --reporter mochawesome --timeout 10000",
    "test:unit": "mocha test/suite/unit.suite.js --reporter mochawesome --timeout 10000",
    "test:login": "mocha test/spec/login.spec.js --timeout 2500",
    "test:regis": "mocha test/spec/registration.js --timeout 2500",
    "html-report:api": "start ./mochawesome-report/mochawesome.html"
  },
  "keywords": [
    "api",
    "automation"
  ],
  "author": "linda",
  "license": "ISC",
  "dependencies": {
    "chai": "^4.3.7",
    "mocha": "^10.2.0",
    "mochawesome": "^7.1.3",
    "supertest": "^6.3.3"
  }
}
